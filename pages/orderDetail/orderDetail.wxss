.fr {
    float: right;
}
.fl {
    float: left;
}

.order_djs {
    height: 36px;
    line-height: 36px;
    padding: 0 15px;
    background: #fff391;
    text-align: center
}

.order_djs text {
    color: #f85800
}

.order_btDay {
    height: 36px;
    line-height: 36px;
    padding: 0 15px;
    background: #f7f8fa;
}

.xq_bt {
    text-indent: 15px;
    height: 44px;
    line-height: 44px;
    border-bottom: 1px solid #e8ebf0;
    padding-right: 15px;
    font-size: 16px;
}

.xq_block {
    padding: 10px 15px;
    background: #fff;
    position: relative;
    z-index: 1;
}

.xq_block .list_li {
    margin: 0;
    position: relative;
    padding: 5px 0 5px 80px;
    line-height: 20px;
    border: 0
}

.xq_block_l {
    width: auto;
    display: block;
    position: absolute;
    left: 0;
    top: 5px;
    color: #848c99;
    text-align: left
}

.xq_bt2 {
    height: 44px;
    line-height: 44px;
    padding-left: 15px;
    padding-right: 15px;
}

.xq_fabud {
    padding: 10px 15px;
    background: #f7f8fa;
    color: #959ba7
}

.po_re{
    position: relative;
}
.order_s3 {
    background: #fff;
    border-bottom: 1px solid #e8ebf0;
}

.fontred1 {
    color: #f00;
}

.new_fbtime {
    padding: 0 15px;
    height: 36px;
    line-height: 36px;
    border-top: 1px solid #e8ebf0;
    color: #ccc
}

.picctxt {
    height: auto;
    line-height: 20px;
    display: block;
    padding: 10px 15px;
}

.picctxt.yuyue {
    padding: 10px 40px 10px 15px;
    position: relative;
}

.picctxt a {
    color: #1cc6a3;
    text-decoration: underline;
}

.xqbtn_foot {
    position: fixed;
    left: 0;
    bottom: 0;
    width: 100%;
    background-color: #fff;
    text-align: right;
    border-top: 1px solid #e8ebf0;
    z-index:999;
}

.xqbtn_foot view {
    border: 1px solid #ddd;
    color: #666;
    border-radius: 5px;
    width: auto;
    padding: 0 10px;
    height: 36px;
    line-height: 36px;
    margin: 17px 10px 17px 0;
    text-align: center;
    display: block;
    float: right
}

.xqbtn_foot view:first-child {
    margin: 17px 10px 17px 0;
}

.xqbtn_foot view.red{
    border: 1px solid #1cc6a3;
    color: #1cc6a3
}


.xq_fabud .p2 {
    text-align: center;
    display: block;
    width: 100%;
    line-height: 40px;
}

.kefuTel text {
    color: #1cc6a3;
}
.picctxt navigator{
    display: inline;
    color:#1cc6a3; 
    text-decoration:underline;
    padding:0 2px;
}
.clearfix:before, .clearfix:after {
    display: table;
    line-height: 0;
    content: "";
    clear:both;
}
.botm-btn {
    height: 74px;
}
.kefuTel {
    display: block;
    line-height: 16px;
    padding: 0 0 10px 0;
    text-align: center;
}
.yiguoqi {
    text-align: center;
    background: #fff;
    padding: 20px 0;
    border-bottom: 1px solid #e8ebf0
}

.yiguoqi view {
    height: 40px;
    line-height: 40px;
    display: inline-block;
    background: url(http://m.yihu365.com/images/guoqi.png) no-repeat left center;
    background-size: 40px 40px;
    padding-left: 50px;
}

.xq_txtr {
    text-align: right;
}

.icophone {
    height: 40px;
    width: 40px;
    position: absolute;
    top: 50%;
    margin-top: -20px;
    right: 20px;
    z-index: 10;
}

.icophone image {
    height: 40px;
    width: 40px;
}

.FW_jilu {
    padding: 0 15px;
    height: 40px;
    line-height: 40px;
    border-bottom: 1px solid #e8ebf0;
    position: relative;
    background: #fff
}

.FW_jilu .fr {
    padding-right: 22px
}
.bar_blank{
    height:10px;
}
.dizhimore {
    background: url(http://m.yihu365.com/images/dizhi03.png) no-repeat scroll 20px center / 18px auto;
    display: block;
    height: 100%;
    position: absolute;
    right: 0;
    top: 0;
    width: 50px;
}
.cancleRadio view{
    margin-top:15px;
    font-size:14px;
}
.cancleRadio radio{
    vertical-align: middle;
    margin-top:-3px;
}
.xqbtn_foot view.noGo{
    background: #999;
    border:medium none;
    padding:0;
}
.xqbtn_foot view.red{
    padding:0;
}
.noGo text{
    color:#ffffff;
}
.xqbtn_foot view.red text,.xqbtn_foot view.noGo text{
    display: block;
    padding:0 10px;
}
.xqbtn_foot view.gotoPay{
    padding:0 10px;
}

